{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://github.com/dymecode/proto-bv/src/schema/fields.json#",
  "title": "IMR field specs",
  "description": "Defines the required structure of object properties when used as a form/table field.",
  "type": "object",
  "properties": {
    "created_at": {
      "type": "string",
      "format": "date-time"
    },
    "priority": {
      "type": "string",
      "enum": ["P1", "P2"]
    },
    "number": {
      "type": "string",
      "format": "^INCd+$"
    }
  },
  "required": ["number"]
}
